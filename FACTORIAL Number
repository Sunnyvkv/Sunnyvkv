import java.util.Scanner;
public class FACTORIAL_No{
public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
int n,i;
int fact=1;
System.out.println("Enter No");
n=sc.nextInt();

for(i=1;i<=n;i++){
fact = fact*i;

}

System.out.println(" FACTORIAL No = "+n+"is = "+fact);
	
	}
}
