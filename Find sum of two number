import java.util.Scanner;
public class Find_Sum{
public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
int i,n;
int sum=0;
//System.out.println("Enter No which table you want from 1 to 10");
n=sc.nextInt();
i=1;
while(i<=n){
sum=sum+i;
i=i+1;
System.out.println(sum);
	}

}
}
