import java.util.Scanner;
public class Revstarpattan{
public static void main(String[] args) {
Scanner sc = new Scanner(System.in);

        int n;
        
        System.out.println("Enter Number");
        n = sc.nextInt();

         for (int i=1;i<=n;n--) 
{
	for(int j=1;j<=n;j++)
{
System.out.print("*");        
System.out.print(j);
    }
System.out.println();
}
}
}
